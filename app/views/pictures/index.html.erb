<h1>Pictures!</h1>
<%= link_to("New Picture", new_picture_path) %>

<% @most_recent_pictures.each do |new_picture| %>
<%= render 'display_picture', picture: new_picture %>
<% end %>

<h2>Highlights from the past</h2>
<% @older_than_month.each do |old_picture| %>
  <%= render 'display_picture', picture: old_picture %>
<% end %>

<h2>View images by year</h2>
<% @years.each do |year| %>
  <p><%= link_to("#{ year }", pictures_year_path(year)) %></p>
<% end %>
